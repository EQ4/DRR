    <item>
      <title>The $3-Trillion War</title>
      <link>http://feedproxy.google.com/~r/TruthdigPodcast/~3/UMlpm47xJkQ/</link>
      <guid isPermaLink="false">http://www.truthdig.com/about/podcasts/#When:21:23:30Z</guid>
